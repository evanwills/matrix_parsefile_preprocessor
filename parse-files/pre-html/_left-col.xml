			<ul class="level2">
					<MySource_ASSET>
				<!-- START: #static_level_left_menu__sub_menu: item -->
				<li class="<MySource_PRINT var='settings.class' />
						   <MySource_PRINT var='asset_type^lowercase^css_safe' />">
