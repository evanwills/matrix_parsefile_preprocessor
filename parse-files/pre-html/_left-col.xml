
<MySource_AREA id_name="metadata_left_menu_prefix" design_area="metadata" print="no">
	<MySource_SET name="metadata_field" value="_content_menu_prefix" />
</MySource_AREA>
<MySource_AREA id_name="metadata_content_menu" design_area="metadata" print="no"><MySource_SET name="metadata_field" value="hide_content_menu" /></MySource_AREA>
<MySource_AREA id_name="showif_metadata_content_menu_class" design_area="show_if" print="no">
	<MySource_SET name="condition" value="keyword_regexp" />
	<MySource_SET name="condition_keyword" value="asset_metadata_hide_content_menu" />
	<MySource_SET name="condition_keyword_match" value="hide(?:-content-menu)" />
	<MySource_THEN><MySource_PRINT id_name="metadata_content_menu" /> </MySource_THEN>
</MySource_AREA>
<MySource_AREA id_name="showif_left_col_metadata_hidden_right" design_area="show_if" print="no">
	<MySource_SET name="condition" value="keyword_regexp" />
	<MySource_SET name="condition_keyword" value="asset_metadata_hide_right_column" />
	<MySource_SET name="condition_keyword_match" value="hide(?:-right-col)?" />
	<MySource_THEN>col-md-pull-9 right-col-not-visible</MySource_THEN>
	<MySource_ELSE>col-md-pull-6 right-col-visible</MySource_ELSE>
</MySource_AREA>
<MySource_AREA id_name="left_menu_prefix_static" design_area="nest_content" print="no"><MySource_SET name="type_codes" value="page" /></MySource_AREA>
<MySource_AREA id_name="left_menu_prefix_dynamic" design_area="nest_content" print="no"><MySource_SET name="type_codes" value="page" /></MySource_AREA>
<MySource_AREA id_name="static_level_left_menu" design_area="menu_normal" print="no">
