						<MySource_PRINT var="asset_short_name" />
					</a>
						<MySource_SUB design_area="menu_normal">
							<MySource_SET name="show_subs" value="on_current" />
							<MySource_SET name="settings.class.normal" value="Sibling"/>
							<MySource_SET name="settings.class.current" value="Nav_Selected"/>
							<MySource_SET name="settings.class.hierarchy" value="Hierarchy" />
							<MySource_SET name="unwanted_asset_types" value="file, news_item, calendar_event_recurring, calendar_event_single, excel_doc, flv_file, image, mp3_file, pdf_file, powerpoint_doc, rtf_file, text_file, video_file, word_doc, xml_file, xsl_file" />
					<!-- START: #static_level_left_menu__sub_menu__sub_menu -->
					<ul class="level3">
								<MySource_ASSET>

						<!-- START: #static_level_left_menu__sub_menu__sub_menu: item -->
						<li class="<MySource_PRINT var='settings.class' />
								   <MySource_PRINT var='asset_type^lowercase^css_safe' />">
							<a href="<MySource_PRINT var='asset_link' />">
								<MySource_PRINT var="asset_short_name" />
